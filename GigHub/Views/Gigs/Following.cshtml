@model GigHub.ViewModels.FollowingViewModel
@{
    ViewBag.Title = "Following";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Following</h1>
<ul class="gigs voffset4">
    @foreach (var following in Model.Following)
    {
        <li>
            <div class="details">
                <span class="artist">
                    @following.Name
                </span>
            </div>
        </li>
    }
</ul>
